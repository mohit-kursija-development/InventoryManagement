  <body>
  <header>
    <!-- Header content here -->
    <nav>
      <div class="head-div">
        <h1>Inventory Management</h1>
      </div>
    </nav>
  </header>
  
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <%= yield %>
      </div>
    </div>
  </div>

  <footer>
    <div class="foot-div">
      <p>&copy; <%= Time.now.year %> Inventory Management</p>
      <p>Project By mkursijadeveloper@gmail.com</p>
    </div>
  </footer>

  
